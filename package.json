    "crypto-js": "^4.2.0",
    "@types/crypto-js": "^4.2.0",
    "bcrypt": "^5.1.1",
    "@types/bcrypt": "^5.0.0",
    "jsonwebtoken": "^9.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "@types/pg": "^8.11.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  },
